<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.curriculum">

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/icona"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icona"
        android:supportsRtl="true"
        android:theme="@style/My_style">
        <activity android:name="com.example.Curriculum.EmailSender" />
        <activity android:name="com.example.Curriculum.Description" />
        <activity android:name="com.example.Curriculum.MainActivity" />
        <activity android:name="com.example.Curriculum.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>